{
  "name": "famicomjs",
  "version": "0.1.0",
  "main": "index.js",
  "author": "Rahadian Yusuf <rahadian.y@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean:web": "shx rm -rf build",
    "clean:bin": "shx rm -rf _bin",
    "clean": "npm run clean:bin && npm run clean:web",
    "start:web": "snowpack dev",
    "start": "npm run start:web",
    "build:web": "npm run clean:web && snowpack build",
    "build:bin": "npm run clean:bin && rollup -c scripts/build/rollup.bin.config.js --environment NODE_ENV:production",
    "build": "npm run build:web && npm run build:bin",
    "generate-opcodes": "node build-opcode.js && eslint --fix src/6502/instructions/index.js",
    "test": "jest --coverage",
    "download-nes-test": "wget -c http://nickmass.com/images/nestest.nes"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/eslint-parser": "^7.11.4",
    "@babel/generator": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-json": "^4.1.0",
    "@snowpack/app-scripts-svelte": "^1.8.5",
    "@snowpack/plugin-babel": "^2.1.0",
    "@snowpack/plugin-dotenv": "^2.0.1",
    "@snowpack/plugin-optimize": "^0.2.3",
    "@snowpack/plugin-run-script": "^2.1.2",
    "@snowpack/plugin-svelte": "^2.0.4",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/svelte": "^3.0.0",
    "@types/jest": "^26.0.10",
    "autoprefixer": "^10.0.1",
    "babel-jest": "^26.3.0",
    "codecov": "^3.7.2",
    "concurrently": "^5.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "glob": "^7.1.6",
    "husky": ">=4",
    "jest": "^26.4.0",
    "jest-canvas-mock": "^2.3.0",
    "lint-staged": ">=10",
    "nollup": "^0.13.5",
    "postcss": "^8.1.1",
    "postcss-cli": "^8.1.0",
    "prettier": "^2.0.5",
    "rollup": "^2.26.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-static-files": "^0.1.0",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-terser": "^7.0.0",
    "serve": "^11.3.2",
    "shx": "^0.3.2",
    "snowpack": "^2.9.3",
    "svelte": "^3.29.0",
    "svelte-check": "^1.0.0"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/6502/assembler.js",
      "!src/6502/instructions/index.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "regenerator-runtime": "^0.13.7",
    "tailwindcss": "^1.8.13",
    "yargs": "^15.4.1"
  }
}
