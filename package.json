{
  "name": "famicomjs",
  "version": "0.1.0",
  "main": "index.js",
  "author": "Rahadian Yusuf <rahadian.y@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:**",
    "dev:frontend": "vite",
    "build": "NODE_ENV=production npm run build:neslib && vite build",
    "serve": "vite preview",
    "build:neslib": "node build-rs.js",
    "dev:neslib": "npm run build:neslib",
    "test": "jest --coverage",
    "generate-cpu": "node scripts/cpu/index.js > src/cpu/index.ts && eslint --cache --fix src/cpu/index.ts",
    "download-nes-test": "wget -c http://nickmass.com/images/nestest.nes",
    "remove-nes-test": "rm -fv nestest.nes",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/eslint-parser": "^7.13.4",
    "@babel/generator": "^7.11.4",
    "@babel/node": "^7.12.6",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.0.1",
    "babel-jest": "^26.3.0",
    "codecov": "^3.7.2",
    "concurrently": "^5.3.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^7.13.0",
    "glob": "^7.1.6",
    "husky": ">=6",
    "jest": "^26.4.0",
    "jest-canvas-mock": "^2.3.0",
    "lint-staged": ">=10",
    "mustache": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.1",
    "postcss-cli": "^8.1.0",
    "prettier": "^2.0.5",
    "rollup": "^2.26.2",
    "serve": "^11.3.2",
    "shx": "^0.3.2",
    "typescript": "^4.1.3",
    "vite": "^2.0.1",
    "yaml": "^1.10.0"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.ts",
      "src/**/*.vue"
    ]
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "gamecontroller.js": "^1.5.0",
    "lodash": "^4.17.21",
    "lodash.camelcase": "^4.3.0",
    "regenerator-runtime": "^0.13.7",
    "tailwindcss": "^1.8.13",
    "vue": "^3.0.5",
    "yargs": "^15.4.1"
  }
}
